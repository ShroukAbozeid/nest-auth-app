<div class="mdl-layout mdl-js-layout mdl-color--grey-100">
  <main class="mdl-layout__content">
    <div class="mdl-card mdl-shadow--6dp" style="width:100%">
      <div class="mdl-card__title mdl-color--primary mdl-color-text--white">
        <h2 class="mdl-card__title-text">All Users</h2>
      </div>
      <table>
        <tr>
          <th>Id</th><th>Name</th><th>Email</th>
          <th>Email Confirmed</th><th>Provider</th><th>Admin</th>
          <th>Creation time</th><th>Last update at</th>
        </tr>
        {{#each users}}
          <tr>
            <th>{{id}}</th>
            <th>{{name}}</th>
            <th>{{email}}</th>
            <th>{{emailConfirmed}}</th>
            <th>{{provider}}</th>
            <th>{{admin}}</th>
            <th>{{createdAt}}</th>
            <th>{{updatedAt}}</th>
          </tr>
        {{/each}}
      </table>
      <a class="mdl-button" href='/home'>back</a>
    </div>
  </main>
</div>